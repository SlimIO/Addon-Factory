{
  "name": "@slimio/addon-factory",
  "version": "0.4.0",
  "description": "SlimIO Addon Factory",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env npm test && eslint src/**",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/AddonFactory.git"
  },
  "keywords": [
    "SlimIO",
    "Factory",
    "Addon",
    "Mock"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/AddonFactory/issues"
  },
  "homepage": "https://github.com/SlimIO/AddonFactory#readme",
  "dependencies": {
    "@slimio/is": "^1.5.0",
    "@slimio/utils": "^0.8.0",
    "is-snake-case": "^0.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^2.0.3",
    "@types/rimraf": "^2.0.2",
    "ava": "^1.4.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "jsdoc": "^3.5.5",
    "nyc": "^13.3.0",
    "pkg-ok": "^2.3.1",
    "rimraf": "^2.6.3"
  }
}
